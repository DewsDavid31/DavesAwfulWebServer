prefix=/Applications/AMPPS/apps/apache
exec_prefix=/Applications/AMPPS/apps/apache
libdir=${prefix}/lib
APRUTIL_MAJOR_VERSION=1
includedir=/Applications/AMPPS/apps/apache/include

Name: APR Utils
Description: Companion library for APR
Version: 1.6.1
# assume that apr-util requires libapr of same major version
Requires: apr-1
Libs: -L${libdir} -laprutil-${APRUTIL_MAJOR_VERSION}  -lexpat -liconv
Cflags: -I${includedir}
