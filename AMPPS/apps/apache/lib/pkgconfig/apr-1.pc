prefix=/Applications/AMPPS/apps/apache
exec_prefix=/Applications/AMPPS/apps/apache
libdir=${prefix}/lib
APR_MAJOR_VERSION=1
includedir=/Applications/AMPPS/apps/apache/include

Name: APR
Description: The Apache Portable Runtime library
Version: 1.7.0
Libs: -L${libdir} -lapr-${APR_MAJOR_VERSION} -lpthread
Cflags:  -DDARWIN -DSIGPROCMASK_SETS_THREAD_MASK -DDARWIN_10 -g -O2 -I${includedir}
